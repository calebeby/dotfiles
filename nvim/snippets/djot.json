{
  "$": {
    "body": "\\$`${1:}`$0",
    "prefix": "$"
  },
  "$$": {
    "body": "\\$\\$`${1:}`$0",
    "prefix": "$$"
  },
  ":::": {
    "body": [
      "::: ${1:}",
      ":::$0"
    ],
    "prefix": ":::"
  },
  "::::": {
    "body": [
      ":::: ${1:}",
      "::::$0"
    ],
    "prefix": "::::"
  },
  "AP Question": {
    "body": [
      "1. ${2:Question}",
      "",
      "  ::: columns-2",
      "  a. ${3:Option}",
      "  :::",
      "",
      "  {% Solution: ${4:Solution} %}",
      "  {% Source: College Board: ${1:Name} %}",
      "$0"
    ],
    "prefix": "AP MCQ"
  },
  "Box Method": {
    "body": [
      "```=typ",
      "#import \"graphs.typ\": box_method, figure",
      "#show: figure",
      "#box_method(top: \\$${3:x + 5}\\$, left: \\$${4:x - 5}\\$)",
      "```"
    ],
    "prefix": "Box Method"
  },
  "FRQ rubric level": {
    "body": [
      "## FRQ #1 Part ${1:A} - ${2:Title}",
      "",
      ": Proficient (1)",
      "",
      "  ${3: 1 point is earned for...}",
      "",
      ": Not Proficient (0)",
      "$0"
    ],
    "prefix": "FRQ rubric level"
  },
  "Rubric Level Boolean": {
    "body": [
      "## ${1:1}: ${2:Title}",
      "",
      ": Proficient (1)",
      "",
      "  $3",
      "",
      ": Not Proficient (0)",
      "$0"
    ],
    "prefix": "rubric level boolean"
  },
  "Say hello to the world": {
    "body": [
      ":::: slide",
      "${1:}",
      "::::$0"
    ],
    "prefix": "slide"
  },
  "TODO": {
    "body": "{% TODO: ${1:} %}$0",
    "prefix": "TODO"
  },
  "ab": {
    "body": [
      "::: answer-box",
      "$1",
      ":::"
    ],
    "prefix": "ab"
  },
  "answer-box": {
    "body": [
      "::: answer-box",
      ":::"
    ],
    "prefix": "answer-box"
  },
  "comment": {
    "body": [
      "::: comment",
      "${1:}",
      ":::$0"
    ],
    "prefix": "comment"
  },
  "embed": {
    "body": [
      "{embed=\"${1:../path/to/file.dj}\"}",
      ":::",
      ":::",
      "$0"
    ],
    "prefix": "embed"
  },
  "float-right + fr": {
    "body": [
      "${1::::} float-right",
      "$2",
      "$1",
      "",
      "$0"
    ],
    "prefix": [
      "float-right",
      "fr"
    ]
  },
  "graph": {
    "body": [
      "```=typ",
      "#import \"graphs.typ\": graph, plot, figure",
      "#show: figure",
      "#graph(",
      "  size: (4, 4),",
      "  {",
      "    ${1:plot.add()}",
      "  },",
      ")",
      "```"
    ],
    "prefix": "graph"
  },
  "numberline": {
    "body": [
      "```=typ",
      "#import \"graphs.typ\": number-line, figure",
      "#show: figure",
      "#number-line(min: -10, max: 10, size: 12)",
      "```"
    ],
    "prefix": "numberline"
  },
  "practice problem": {
    "body": [
      "{.start-new-page}",
      "# Problem ${1:1}",
      "",
      "${2:}",
      "",
      "{.start-new-page}",
      "# Problem $1 Solution",
      "",
      "{% Source: ${3:} %}",
      "",
      "${4:}"
    ],
    "prefix": "practice problem"
  },
  "problem-columns + pcs": {
    "body": [
      ":::: problem-columns",
      "${1:}",
      "::::$0"
    ],
    "prefix": "problem-columns"
  },
  "problem-columns-full-width": {
    "body": [
      ":::: problem-columns-full-width",
      "${1:}",
      "::::$0"
    ],
    "prefix": "problem-columns-full-width"
  },
  "pu": {
    "body": "\\$`\\pu{${1:10 m}}`",
    "prefix": "pu"
  },
  "rs4": {
    "body": [
      "## ${1:1}: ${2:Title}",
      "",
      ": Proficient (3)",
      "",
      "  $3",
      "",
      ": Correct Answer, Not Enough Work Shown (2)",
      "",
      ": Almost There (2)",
      "",
      "  ${4:Minor mistake prevented student from reaching correct answer}",
      "",
      ": Starting (1)",
      "",
      ": Missing (0)",
      "$0"
    ],
    "prefix": "rubric level"
  },
  "sinfo": {
    "body": [
      "::: student-info",
      ":::"
    ],
    "prefix": "student-info"
  },
  "solution + sol": {
    "body": "{% Solution: ${1:} %}",
    "prefix": "solution"
  },
  "source": {
    "body": "{% Source: $1 %}",
    "prefix": "source"
  },
  "ws": {
    "body": [
      "::: work-space-height-${1:2}em",
      ":::$0"
    ],
    "prefix": "ws"
  },
  "{%": {
    "body": "{% $1 %",
    "prefix": "{%"
  }
}
