{
  "$": {
    "body": "\\$`${1:}`$0",
    "prefix": "$"
  },
  "$$": {
    "body": "\\$\\$`${1:}`$0",
    "prefix": "$$"
  },
  ":::": {
    "body": [
      "::: ${1:}",
      ":::$0"
    ],
    "prefix": ":::"
  },
  "::::": {
    "body": [
      ":::: ${1:}",
      "::::$0"
    ],
    "prefix": "::::"
  },
  "Say hello to the world": {
    "body": [
      ":::: slide",
      "${1:Slide contents}",
      "::::",
      "",
      "$0"
    ],
    "prefix": [
      "slide",
      "sl"
    ]
  },
  "TODO": {
    "body": "{% TODO: ${1:} %}$0",
    "prefix": "TODO"
  },
  "float-right + fr": {
    "body": [
      "${1::::} float-right",
      "$2",
      "$1",
      "",
      "$0"
    ],
    "prefix": [
      "float-right",
      "fr"
    ]
  },
  "graph": {
    "body": [
      "```=typ",
      "#import \"graphs.typ\": graph, unit, plot, figure",
      "#show: figure",
      "#graph(",
      "  size: (4, 4),",
      "  {",
      "    ${1:plot.add()}",
      "  },",
      ")",
      "```"
    ],
    "prefix": "graph"
  },
  "problem-columns + pcs": {
    "body": [
      ":::: problem-columns",
      "${1:}",
      "::::$0"
    ],
    "prefix": [
      "problem-columns",
      "pc"
    ]
  },
  "solution + sol": {
    "body": "{% Solution: ${1:} %}",
    "prefix": [
      "solution",
      "sol"
    ]
  },
  "ws": {
    "body": [
      "::: work-space-height-${1:2}em",
      ":::$0"
    ],
    "prefix": "ws"
  }
}
