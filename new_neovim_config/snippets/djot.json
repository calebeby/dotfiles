{
  "$": {
    "body": "\\$`${1:}`$0",
    "prefix": "$"
  },
  "$$": {
    "body": "\\$\\$`${1:}`$0",
    "prefix": "$$"
  },
  ":::": {
    "body": [
      "::: ${1:}",
      ":::$0"
    ],
    "prefix": ":::"
  },
  "::::": {
    "body": [
      ":::: ${1:}",
      "::::$0"
    ],
    "prefix": "::::"
  },
  "Say hello to the world": {
    "body": [
      ":::: slide",
      "${1:Slide contents}",
      "::::$0"
    ],
    "prefix": [
      "slide",
      "sl"
    ]
  },
  "TODO": {
    "body": "{% TODO: ${1:} %}$0",
    "prefix": "TODO"
  },
  "ab": {
    "body": [
      "::: answer-box",
      "$1",
      ":::"
    ],
    "prefix": "ab"
  },
  "float-right + fr": {
    "body": [
      "${1::::} float-right",
      "$2",
      "$1",
      "",
      "$0"
    ],
    "prefix": [
      "float-right",
      "fr"
    ]
  },
  "graph": {
    "body": [
      "```=typ",
      "#import \"graphs.typ\": graph, unit, plot, figure",
      "#show: figure",
      "#graph(",
      "  size: (4, 4),",
      "  {",
      "    ${1:plot.add()}",
      "  },",
      ")",
      "```"
    ],
    "prefix": "graph"
  },
  "numberline": {
    "body": [
      "```=typ",
      "#import \"graphs.typ\": number-line, figure",
      "#show: figure",
      "#number-line(min: -10, max: 10, size: 12)",
      "```"
    ],
    "prefix": "numberline"
  },
  "problem-columns + pcs": {
    "body": [
      ":::: problem-columns",
      "${1:}",
      "::::$0"
    ],
    "prefix": [
      "problem-columns",
      "pc"
    ]
  },
  "pu": {
    "body": "\\$`\\pu{${1:10 m}}`",
    "prefix": "pu"
  },
  "rs4": {
    "body": [
      "## ${1:1}: ${2:Title}",
      "",
      ": Proficient (4)",
      "",
      "  $3",
      "",
      ": Almost There (3)",
      "",
      "  ${4:Minor mistake, or correct answer with work not shown.}",
      "",
      ": Starting (2)",
      "",
      ": Missing (1)"
    ],
    "prefix": "rs4"
  },
  "sinfo": {
    "body": [
      "::: student-info",
      ":::"
    ],
    "prefix": "sinfo"
  },
  "solution + sol": {
    "body": "{% Solution: ${1:} %}",
    "prefix": "solution"
  },
  "source": {
    "body": "{% Source: $1 %}",
    "prefix": "source"
  },
  "ws": {
    "body": [
      "::: work-space-height-${1:2}em",
      ":::$0"
    ],
    "prefix": "ws"
  },
  "{%": {
    "body": "{% $1 %",
    "prefix": "{%"
  }
}
